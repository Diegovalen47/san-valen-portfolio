---
const { technologies } = Astro.props;
---

<ul class="mb-3 flex flex-wrap gap-2">
  {technologies.map((technology: string) => (
    <li>
      <span class="bg-blue-100 text-blue-800 text-xs font-medium px-3 py-1 rounded-full dark:bg-gray-700 dark:text-blue-400 border border-blue-400">{technology}</span>
    </li>
  ))}
</ul>